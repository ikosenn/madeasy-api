Program:
    commands*=Command
;

Command:
    BookCommand | ShowCommand // | SearchCommand
;

BookCommand:
    'show me flights to' country_arrive=Country 'from' country_departure=Country 'on' date_leave=Date ('to' date_return=Date)?
;

ShowCommand:
    'show me my booked flights'
;

Country:
    /[a-zA-Z]+/
;

Date:
    /(0[1-9]|[12][0-9]|3[01])([-\/.])(0[1-9]|1[012])\2(19|20)\d\d/
;
