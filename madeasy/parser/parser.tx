Program:
    commands*=Command
;

Command:
    BookCommand // | SearchCommand | ShowCommand
;

BookCommand:
    'book a flight to' country_arrive=Country 'from' country_departure=Country 'on' date_leave=Date ('to' date_return=Date)?
;


Country:
    /[a-zA-Z]+/
;

Date:
    /(0[1-9]|[12][0-9]|3[01])([-\/.])(0[1-9]|1[012])\2(19|20)\d\d/
;
